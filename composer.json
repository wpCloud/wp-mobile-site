{
    "name": "wpcloud/wp-mobile-site",
    "type": "wordpress-plugin",
    "description": "Handler for mobile sites.",
    "require": {
        "composer/installers": "~1.0"
    },
    "keywords": [ "wordpress", "wpcloud", "network", "mobile" ],
    "license": "GPL-3.0+",
    "repositories": [
      {
        "type": "composer",
        "url": "https://repository.usabilitydynamics.com"
      },
      {
        "type": "composer",
        "url": "http://wpackagist.org"
      }
    ],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "andypotanin",
            "email": "andy.potanin@usabilitydynamics.com"
        }
    ],
    "config": {
      "component-dir": "static/assets/vendor",
      "github-protocols": [ "ssh" ],
      "vendor-dir": "vendor",
      "bin-dir": "vendor/.bin",
      "discard-changes": true,
      "optimize-autoloader": true
    }
}
